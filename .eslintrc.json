{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": [
        "**/dist/*.js"
    ],
    "rules": {
        "linebreak-style": "off",
        "no-tabs": "off",
        "indent": [
            "off",
            "tab"
        ],
        "max-len": [
            "error",
            {
                "code": 240
            }
        ]
    }
}